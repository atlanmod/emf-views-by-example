<?xml version="1.0" encoding="UTF-8"?>
<virtualLinks:WeavingModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:virtualLinks="http://www.atlanmod.org/emfviews/virtuallinks/0.3.0"
    name="virtualAssociationExample">
  <virtualLinks
      xsi:type="virtualLinks:VirtualAssociation"
      name="bookCode"
      source="//@contributingModels.0/@concreteElements.0"
      target="//@contributingModels.1/@concreteElements.0"
      lowerBound="1"
      upperBound="1"/>
  <virtualLinks
      xsi:type="virtualLinks:VirtualAssociation"
      name="publicationISBN"
      source="//@contributingModels.1/@concreteElements.0"
      target="//@contributingModels.0/@concreteElements.0"
      upperBound="-1"
      opposite="//@virtualLinks.0"/>
  <contributingModels
      URI="http://publication">
    <concreteElements
        xsi:type="virtualLinks:ConcreteConcept"
        path="Publication"/>
  </contributingModels>
  <contributingModels
      URI="http://book">
    <concreteElements
        xsi:type="virtualLinks:ConcreteConcept"
        path="Book"/>
  </contributingModels>
</virtualLinks:WeavingModel>
